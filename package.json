{
  "name": "nestjs-example",
  "version": "0.0.2",
  "description": "",
  "author": "t.kosminov",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start:dev": "nodemon",
    "start:build": "node --enable-source-maps dist/main.js --unhandled-rejections=warn",
    "repl:dev": "NODE_ENV=development ts-node --files -r tsconfig-paths/register src/repl.ts --unhandled-rejections=warn",
    "repl:build": "node --enable-source-maps dist/repl.js --unhandled-rejections=warn",
    "typeorm:cli:ts": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:create": "NODE_ENV=development npm run typeorm:cli:ts -- migration:create ./src/typeorm/migrations/created",
    "typeorm:generate": "NODE_ENV=development npm run typeorm:cli:ts -- -d ./src/typeorm/typeorm.cli.ts migration:generate ./src/typeorm/migrations/generated",
    "typeorm:run:dev": "NODE_ENV=development npm run typeorm:cli:ts -- -d ./src/typeorm/typeorm.cli.ts migration:run",
    "typeorm:cli:js": "node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:run:build": "npm run typeorm:cli:js -- -d ./dist/typeorm/typeorm.cli.js migration:run",
    "madge:full": "madge --image graph.png --extensions ts src",
    "madge:circular": "madge --image graph.png --extensions ts --circular src",
    "prepare": "husky"
  },
  "dependencies": {
    "@envelop/core": "^5.0.2",
    "@envelop/response-cache": "^6.2.4",
    "@golevelup/nestjs-rabbitmq": "^5.5.0",
    "@graphql-tools/delegate": "^10.1.0",
    "@graphql-tools/stitch": "^9.3.2",
    "@graphql-tools/stitching-directives": "^3.1.11",
    "@graphql-tools/utils": "^10.5.5",
    "@graphql-yoga/nestjs": "^3.7.0",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/microservices": "^10.4.4",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/platform-socket.io": "^10.4.6",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.6",
    "@socket.io/redis-adapter": "^8.3.0",
    "@whatwg-node/fetch": "^0.9.22",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "graphql": "^16.9.0",
    "graphql-redis-subscriptions": "^2.6.1",
    "graphql-scalars": "^1.23.0",
    "graphql-upload-ts": "^2.1.2",
    "graphql-ws": "^5.16.0",
    "hasha": "^5.2.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nestjs-graphql-easy": "^3.1.4",
    "node-abort-controller": "^3.1.1",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.0",
    "transliteration": "^2.3.5",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.4",
    "@types/nodemon": "^1.19.6",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "madge": "^8.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
