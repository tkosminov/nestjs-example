{
  "name": "nestjs-example",
  "version": "0.0.1",
  "description": "",
  "author": "t.kosminov",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node --files -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev": "nodemon",
    "start:build": "NODE_ENV=development node -r ts-node/register -r tsconfig-paths/register dist/main.js",
    "start:repl": "ts-node --files -r tsconfig-paths/register src/repl.ts",
    "start:build:repl": "NODE_ENV=development node -r ts-node/register -r tsconfig-paths/register dist/repl.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "db:migration:create": "npm run typeorm:cli -- migration:create ./src/database/migrations/created",
    "db:migration:generate": "npm run typeorm:cli -- -d ./src/database/database-ormconfig.cli.ts migration:generate ./src/database/migrations/generated",
    "db:migration:run": "npm run typeorm:cli -- -d ./src/database/database-ormconfig.cli.ts migration:run",
    "db:schema:sync": "npm run typeorm:cli -- -d ./src/database/database-ormconfig.cli.ts schema:sync",
    "prepare": "husky install"
  },
  "dependencies": {
    "@graphql-tools/stitch": "^8.7.36",
    "@graphql-tools/wrap": "^9.3.1",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@socket.io/redis-adapter": "^8.0.1",
    "amqp-connection-manager": "^4.1.10",
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.11.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.8",
    "cookie-parser": "^1.4.6",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.6.0",
    "hasha": "^5.2.2",
    "helmet": "^6.0.1",
    "ioredis": "^5.2.5",
    "jsonwebtoken": "^9.0.0",
    "lodash.set": "^4.3.2",
    "nestjs-graphql-easy": "^3.0.0",
    "node-abort-controller": "^3.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.0",
    "rxjs": "^7.8.0",
    "socket.io": "^4.5.4",
    "transliteration": "^2.3.5",
    "typeorm": "^0.3.11",
    "typeorm-extension": "^2.4.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.3",
    "@types/amqplib": "^0.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "^18.11.18",
    "@types/nodemon": "^1.19.2",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
